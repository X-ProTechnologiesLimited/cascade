											.hover: a.lightbox-images(href="images/2250x1500.png"): span.wicon-iconmonstr-zoom-in-thin
											img(src="images/2250x1500.png" alt="")
										.details
											p.title: a(href="#") Crazy Eddie
											p.text-decor-1 New York, NY 10013
							.row
								.col-lg-12
									<!-- Pagination start -->
									include include/pagination.jade
									<!-- Pagination end -->
					<!-- Page content end -->

					<!-- CTA start -->
					.content-section.image-bg.image-bg-8.cta.v3
						.container
							.row
								.col-lg-12.cta-v3-content
									.content
										p.text-1 our mission is to
										p.text-2 boost your revenue & profit
										p We help our clients grow their revenue & profits. Faster, better and sustainably than anyone else.
									p.right: a.btn.btn-1(href="#") <i class="fa fa-file"></i> get a quotation
